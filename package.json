{
    "name": "sqf",
    "displayName": "%displayName%",
    "description": "%description%",
    "version": "0.6.1",
    "publisher": "vlad333000",
    "license": "GPL-3.0-or-later",
    "homepage": "https://github.com/vlad333000/vscode-sqf/blob/master/README.md",
    "bugs": {
        "url": "https://github.com/vlad333000/vscode-sqf/issues",
        "email": "vladsazonov@hotmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/vlad333000/vscode-sqf.git"
    },
    "engines": {
        "vscode": "*"
    },
    "categories": [
        "Programming Languages",
        "Snippets",
        "Themes"
    ],
    "keywords": [
        "sqf",
        "Arma",
        "Arma 3"
    ],
    "contributes": {
        "languages": [
            {
                "id": "sqf",
                "extensions": [
                    ".sqf"
                ],
                "aliases": [
                    "SQF",
                    "sqf",
                    "Status Quo Function"
                ],
                "configuration": "./configs/sqf.language-configuration.json"
            },
            {
                "id": "arma-cfg",
                "extensions": [
                    ".hpp",
                    ".inc"
                ],
                "filenames": [
                    "config.cpp",
                    "extension.ext",
                    "missing.sqm"
                ],
                "aliases": [
                    "arma-cfg",
                    "arma config"
                ],
                "configuration": "./configs/arma-cfg.language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "sqf",
                "scopeName": "source.sqf",
                "path": "./syntaxes/sqf.tmLanguage.json"
            },
            {
                "language": "arma-cfg",
                "scopeName": "source.arma-cfg",
                "path": "./syntaxes/arma-cfg.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "sqf",
                "path": "./snippets/sqf.json"
            },
            {
                "language": "arma-cfg",
                "path": "./snippets/arma-cfg.json"
            }
        ],
        "themes": [
            {
                "id": "SQF-Dark",
                "label": "[SQF] Dark",
                "uiTheme": "vs-dark",
                "path": "./themes/sqf.dark.json"
            },
            {
                "id": "SQF-Light",
                "label": "[SQF] Light",
                "uiTheme": "vs",
                "path": "./themes/sqf.light.json"
            },
            {
                "id": "SQF-High-contrast-black",
                "label": "[SQF] High Contrast (Black)",
                "uiTheme": "hc-black",
                "path": "./themes/sqf.hc.black.json"
            }
        ]
    }
}
